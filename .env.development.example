MODE="development"

# Excalidraw's free encrypted backend services
VITE_APP_BACKEND_V2_GET_URL=https://json-dev.excalidraw.com/api/v2/
VITE_APP_BACKEND_V2_POST_URL=https://json-dev.excalidraw.com/api/v2/post/

# Library browsing - free public service
VITE_APP_LIBRARY_URL=https://libraries.excalidraw.com
# VITE_APP_LIBRARY_BACKEND=https://us-central1-excalidraw-room-persistence.cloudfunctions.net/libraries

# Collaboration WebSocket server - local development server
VITE_APP_WS_SERVER_URL=http://localhost:3002

# DISABLED: Excalidraw Plus services (not needed)
# VITE_APP_PLUS_LP=https://plus.excalidraw.com
# VITE_APP_PLUS_APP=http://localhost:3000

# AI backend - run locally if needed
# VITE_APP_AI_BACKEND=http://localhost:3015

# Appwrite - for collaboration database and file storage
# Replace YOUR_PROJECT_ID with your actual Appwrite Project ID
VITE_APP_APPWRITE_ENDPOINT=https://sgp.cloud.appwrite.io/v1
VITE_APP_APPWRITE_PROJECT_ID=YOUR_PROJECT_ID
VITE_APP_APPWRITE_DATABASE_ID=YOUR_DATABASE_ID
VITE_APP_APPWRITE_TABLE_ID=YOUR_TABLE_ID
VITE_APP_APPWRITE_BUCKET_ID=YOUR_BUCKET_ID

# DISABLED: Firebase (migrated to Appwrite)
# VITE_APP_FIREBASE_CONFIG='{"apiKey":"YOUR_API_KEY","authDomain":"YOUR_AUTH_DOMAIN","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_STORAGE_BUCKET","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID"}'"

# put these in your .env.local, or make sure you don't commit!
# must be lowercase `true` when turned on
#
# whether to disable live reload / HMR. Usually what you want to do when
# debugging Service Workers.
VITE_APP_DEV_DISABLE_LIVE_RELOAD=
VITE_APP_ENABLE_TRACKING=true

FAST_REFRESH=false

# The port to run the dev server
VITE_APP_PORT=3000

# Debug flags

# To enable bounding box for text containers
VITE_APP_DEBUG_ENABLE_TEXT_CONTAINER_BOUNDING_BOX=

# Set this flag to false if you want to open the overlay by default
VITE_APP_COLLAPSE_OVERLAY=true

# Set this flag to false to disable eslint
VITE_APP_ENABLE_ESLINT=true

# Enable PWA in dev server
VITE_APP_ENABLE_PWA=false

# DISABLED: Excalidraw Plus export key
# VITE_APP_PLUS_EXPORT_PUBLIC_KEY='YOUR_PUBLIC_KEY'

# set to true in .env.development.local to disable the prevent unload dialog
VITE_APP_DISABLE_PREVENT_UNLOAD=
