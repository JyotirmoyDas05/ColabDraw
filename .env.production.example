MODE="production"

# Excalidraw's free encrypted backend services
VITE_APP_BACKEND_V2_GET_URL=https://json.excalidraw.com/api/v2/
VITE_APP_BACKEND_V2_POST_URL=https://json.excalidraw.com/api/v2/post/

# Library browsing - free public service
VITE_APP_LIBRARY_URL=https://libraries.excalidraw.com
# VITE_APP_LIBRARY_BACKEND=https://us-central1-excalidraw-room-persistence.cloudfunctions.net/libraries

# Collaboration WebSocket server - free encrypted service
VITE_APP_WS_SERVER_URL=https://oss-collab.excalidraw.com

# DISABLED: Excalidraw Plus services (not needed)
# VITE_APP_PLUS_LP=https://plus.excalidraw.com
# VITE_APP_PLUS_APP=https://app.excalidraw.com

# DISABLED: AI backend
# VITE_APP_AI_BACKEND=https://oss-ai.excalidraw.com

# Appwrite - for collaboration database and file storage
# Replace YOUR_PROJECT_ID with your actual Appwrite Project ID
VITE_APP_APPWRITE_ENDPOINT=https://sgp.cloud.appwrite.io/v1
VITE_APP_APPWRITE_PROJECT_ID=YOUR_PROJECT_ID
VITE_APP_APPWRITE_DATABASE_ID=YOUR_DATABASE_ID
VITE_APP_APPWRITE_TABLE_ID=YOUR_TABLE_ID
VITE_APP_APPWRITE_BUCKET_ID=YOUR_BUCKET_ID

# DISABLED: Firebase (migrated to Appwrite)
# VITE_APP_FIREBASE_CONFIG='{"apiKey":"YOUR_API_KEY","authDomain":"YOUR_AUTH_DOMAIN","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_STORAGE_BUCKET","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID"}'

VITE_APP_ENABLE_TRACKING=false

# DISABLED: Excalidraw Plus export key
# VITE_APP_PLUS_EXPORT_PUBLIC_KEY='YOUR_PUBLIC_KEY'

# Set the below flags explicitly to false in production mode since vite loads and merges .env.local vars when running the build command
VITE_APP_DEBUG_ENABLE_TEXT_CONTAINER_BOUNDING_BOX=false
VITE_APP_COLLAPSE_OVERLAY=false
# Enable eslint in dev server
VITE_APP_ENABLE_ESLINT=false
